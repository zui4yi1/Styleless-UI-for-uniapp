{
  "extends": "@vue/tsconfig/tsconfig.json",
  "compilerOptions": {
    "ignoreDeprecations": "5.0",
    "sourceMap": true,
    "baseUrl": ".",

    "lib": ["esnext", "dom"],
    "types": ["@dcloudio/types", "@types/node"],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["src/**/*.ts", "src/**/*.vue", "typings/*.d.ts", "src/typings/*.d.ts"]
}
